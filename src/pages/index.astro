---
const target = '/es';
---

<!doctype html>
<html lang="es" style="color-scheme: dark;">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="refresh" content={`0;url=${target}`} />
    <script is:inline>
      window.location.replace('/es');
    </script>
    <title>Redirecting...</title>
  </head>
  <body></body>
</html>
